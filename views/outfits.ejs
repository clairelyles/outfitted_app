<% include partials/header %>

	<div class='container-content'>	
		<h2>My Outfits</h2>
		<div class='row'>
			<% for (var i = 0; i < outfits.length; i++){ %>
				<div class='col-xs-4 col-md-4 polariod-outer'>
					<div class='polariod-inner text-center'>
						<span class="glyphicon glyphicon-remove text-center" data-id=<%= outfits[i].id %>></span>	
							<img src='http://res.cloudinary.com/drxybow0v/image/upload/c_fill,h_0.5,r_5,w_0.5/piece_<%= outfits[i].pieces[0].id %>' class='img-responsive'>
						<br />
							<img src='http://res.cloudinary.com/drxybow0v/image/upload/c_fill,h_0.5,r_5,w_0.5/piece_<%= outfits[i].pieces[1].id %>' class='img-responsive'>
					</div>
					<h4><strong><%= outfits[i].tag.tag_name %></strong></h4>
				</div>
			<% } %>	
		</div>
	</div>







<% include partials/footer %>